// generate polynoms from int-iterator
auto generatePolynom = [](int value) -> Polynom<_PolySize, _BaseValue>{
    Polynom<_PolySize, _BaseValue> toReturn;
    for (int i = 0; value; ++i){
        toReturn.m_Data[i] = value % _BaseValue;
        value /= _BaseValue;
    }
    return toReturn;
};
